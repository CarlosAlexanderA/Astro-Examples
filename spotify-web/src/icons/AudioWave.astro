---
interface Props {
  size?: number;
}

const {size} = Astro.props;
---

<svg
  class="transform rotate-90"
  aria-hidden="true"
  width={size || 20}
  height={size || 20}
  role="img"
  aria-hidden="true"
  viewBox="0 0 24 24"
>
  <path
    stroke="currentColor"
    stroke-linecap="round"
    stroke-linejoin="round"
    stroke-width="2"
    d="M8 6h8M6 10h12M8 14h8M6 18h12"></path>
</svg>
